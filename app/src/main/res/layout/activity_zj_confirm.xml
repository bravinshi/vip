<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activity.brandcompany.ZjConfirmActivity"
        android:background="#F2F2F2">
        <ImageView
            android:id="@+id/top_iv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:scaleType="fitXY"
            app:layout_constraintTop_toTopOf="parent" />
        <View
            android:id="@+id/back_v"
            android:layout_width="50dp"
            android:layout_height="50dp"
            app:layout_constraintLeft_toLeftOf="@id/top_iv"
            app:layout_constraintTop_toTopOf="@id/top_iv"></View>

        <ImageView
            android:id="@+id/commit_iv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="50dp"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:scaleType="fitXY"
            android:adjustViewBounds="true"
            android:src="@mipmap/zj_xian_btn"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/data_item_circle_focus"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="1月31日"
            android:textSize="16sp"
            android:textColor="@color/color_333333"
            android:paddingTop="15dp"
            android:paddingBottom="15dp"
            android:paddingLeft="10dp"
            android:visibility="gone"
            />

        <ImageView
            android:id="@+id/iv_item_circle_focus"
            app:layout_constraintTop_toBottomOf="@+id/data_item_circle_focus"
            android:layout_width="match_parent"
            android:adjustViewBounds="true"
            android:layout_height="wrap_content"/>
        <View
            android:id="@+id/v_clicked"
            android:layout_width="90dp"
            android:layout_height="35dp"
            app:layout_constraintRight_toRightOf="@id/iv_item_circle_focus"
            app:layout_constraintBottom_toBottomOf="@+id/iv_item_circle_focus"
            android:layout_marginBottom="@dimen/dp_10" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>